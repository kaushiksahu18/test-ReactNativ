{
  "expo": {
    "name": "RNapp",
    "slug": "RNapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "rnapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kaimama.RNapp"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "package": "com.kaimama.RNapp"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "4955bfc4-e2c5-46d0-891f-a6fc45d5871b"
      }
    }
  }
}
